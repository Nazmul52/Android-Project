<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Photo_Gallery"
    tools:showIn="@layout/activity_photo__gallery">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="108dp"
            android:layout_height="wrap_content"

            app:srcCompat="@drawable/alumni" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="108dp"
            android:layout_height="wrap_content"

            app:srcCompat="@drawable/intconfer" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="108dp"
            android:layout_height="wrap_content"

            app:srcCompat="@drawable/eidreuinon" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="436dp"
        android:orientation="horizontal">



            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="115dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/anniversary" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="115dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/literature" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="115dp"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/orientation" />

    </LinearLayout>

</LinearLayout>


